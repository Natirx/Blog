<h1 id="artical_title">Stuff i Design
  <% if user_signed_in? %>

      <%= link_to "Add Project", new_project_path(@post), class: "add_links" %>

  <% end %>
</h1>

<div id="posts_wrapper" class="skinny_wrapper">
  <% @projects.each do |project| %>
      <div class="post">
        <p class="date"><%= project.created_at.strftime('%A, %b %d') %></p>
        <h2><a href="#"><%= link_to project.title, project %></a></h2>
        <hr>
      </div>
  <% end %>


</div>
